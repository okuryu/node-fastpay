    "request": "2.x.x"
  },
  "devDependencies": {
    "chai": "1.x.x",
    "coveralls": "2.x.x",
    "echoecho": "0.x.x",
    "istanbul": "0.x.x",
    "jshint": "2.x.x",
    "mocha": "1.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:okuryu/node-fastpay.git"
